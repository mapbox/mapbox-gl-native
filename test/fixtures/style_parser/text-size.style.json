{
  "version": 8,
  "sources": {
    "mapbox": {
      "type": "vector",
      "url": "mapbox://mapbox.mapbox-terrain-v1,mapbox.mapbox-streets-v5",
      "maxzoom": 14
    }
  },
  "layers": [{
    "id": "country_label",
    "type": "symbol",
    "source": "mapbox",
    "source-layer": "country_label",
    "filter": ["==", "$type", "Point"],
    "layout": {
      "text-size": "no"
    }
  }]
}
